{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=zena1;Username=admin;Password=12345;TrustServerCertificate=True;"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:3001",
      "http://localhost:50133",
      "http://localhost:5133",
      "https://localhost:7153"
    ]
  },
  "FileStorage": {
    "BaseUrl": "http://localhost:5133"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Information",
        "System": "Information"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/dev-log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  },
  "Loki": {
    "Url": "http://localhost:3100",
    "Labels": {
      "app": "zena-backend",
      "env": "development"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
